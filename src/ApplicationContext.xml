<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-5.0.2.xsd">



<bean id="accountDao" class= "dao.AccountDao" >
	<property name="csvResource" value="csvResource.csv"/>
</bean>

<bean id= "accountService" class="service.AccountService">
	<property name="accountDao" ref="accountDao"/>
</bean>
</beans>